<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="it.hourglass.myTalk.client.shared.Friendships" table="FRIENDSHIPS">
        <id name="friendshipId" type="java.lang.Integer">
            <column name="FRIENDSHIPID" />
            <generator class="identity" />
        </id>
        <property name="friend1" type="java.lang.String">
            <column name="FRIEND1" />
        </property>
        <property name="friend2" type="java.lang.String">
            <column name="FRIEND2" />
        </property>
        <property name="accepted" type="java.lang.Boolean">
            <column name="ACCEPTED" />
        </property>
    </class>
</hibernate-mapping>
